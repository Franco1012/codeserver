{
  "name": "codeserver",
  "version": "1.0.0",
  "description": "En la primera entrega del desaf√≠o, se implementaron dos funciones principales:",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js -p 9090",
    "dev": "nodemon server.js",
    "argv": "node server.js -p 8080 --env=prod",
    "memory": "nodemon server.js  --env=prod --persistence=memory",
    "fs": "node server.js  --env=test --persistence=fs",
    "products": "node ./src/mocks/products.mock.js",
    "users": "node ./src/mocks/users.mock.js",
    "simplex": "artillery quick --count 20 --num 100 http://localhost:8080/simplex -o ./test/artillery/simplex.json",
    "complex": "artillery quick --count 20 --num 100 http://localhost:8080/complex -o ./test/artillery/complex.json",
    "flow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "report": "artillery report ./test/artillery/flow.json",
    "image": "docker build -t matilda .",
    "container": "docker run -p 8888:8080 matilda",
    "rename": "docker tag matilda franco1604/matilda:1.0.0",
    "publish": "docker push franco1604/matilda:1.0.0",
    "orq": "kubectl apply -f ./orq.yaml",
    "minikube": "minikube service matilda"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "artillery-plugin-metrics-by-endpoint": "^1.10.0",
    "bcrypt": "^5.1.1",
    "commander": "^12.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "joi": "^17.13.3",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0"
  }
}
